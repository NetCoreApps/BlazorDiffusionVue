{
    "ConnectionStrings": {
        "DefaultConnection": "DataSource=App_Data/db.sqlite;Cache=Shared",
        "AnalyticsConnection": "DataSource=App_Data/analytics.sqlite;Cache=Shared"
    },
    "Logging": {
        "IncludeScopes": false,
        "Debug": {
            "LogLevel": {
                "Default": "Information"
            }
        },
        "Console": {
            "LogLevel": {
                "Default": "Debug",
                "System": "Information",
                "Microsoft": "Information"
            }
        }
    },
    "AllowedHosts": "*"
}
